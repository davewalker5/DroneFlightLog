<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <PackageId>DroneFlightLog.Data</PackageId>
    <PackageVersion>1.1.0.0</PackageVersion>
    <PackOnBuild>true</PackOnBuild>
    <Authors>Dave Walker</Authors>
    <Copyright>Copyright (c) 2020, 2021, 2022, 2023 Dave Walker</Copyright>
    <Owners>Dave Walker</Owners>
    <PackageReleaseNotes>NuGet package and framework updates</PackageReleaseNotes>
    <Summary>Drone Flight Logging Database Layer</Summary>
    <PackageTags>Drone UAV Log Flight Logbook</PackageTags>
    <Title>Drone Flight Logging Database Layer</Title>
    <Description>Drone Flight Logging Database Layer</Description>
    <PackageProjectUrl>https://github.com/davewalker5/DroneFlightLogDb</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <ReleaseVersion>1.1.0.0</ReleaseVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.10" />
    <PackageReference Include="Microsoft.AspNetCore.Identity" Version="2.2.0" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Entities\" />
    <Folder Include="Interfaces\" />
    <Folder Include="Logic\" />
    <Folder Include="Exceptions\" />
    <Folder Include="Factory\" />
    <Folder Include="Extensions\" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Content\DroneLog.Data.License.txt" />
  </ItemGroup>
</Project>
